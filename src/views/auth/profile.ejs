<%- include('../port/layout-start', {title: title}) %> <%- include('../port/menu') %>

<div class="container">
  <h1><%= title %></h1>

  <div class="row">
    <div class="col-sm-6">
      <p>
        <b>ID:</b> <%= user.id %><br />
        <b>Username:</b> <%= user.username %><br />
        <b>Name:</b> <%= user.displayName %><br />
        <% if (user.emails) { %>
        <b>Email:</b> <%= user.emails[0].value %><br />
        <% } %>
      </p>
    </div>
  </div>
</div>

<%- include('../port/layout-end') %>
